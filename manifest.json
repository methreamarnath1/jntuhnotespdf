    "start_url": "/",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "#1e40af",
    "orientation": "portrait",
    "icons": [
      {
        "src": "path/to/icon-192x192.png",
        "sizes": "192x192",
        "type": "image/png"
      },
      {
        "src": "path/to/icon-192x192.png",
