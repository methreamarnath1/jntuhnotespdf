        "url": "link",
        "files": [
          {
            "name": "file",
            "accept": ["image/*"]
          }
        ]
      }
    },
    "related_applications": [
      {
        "platform": "play",
        "url": "https://play.google.com/store/apps/details?id=com.jntuhnotespdf.vercel.app.app",
        "id": "com.jntuhnotespdf.vercel.app.app"
      }
    ],
    "prefer_related_applications": false,
    "scope": "/"
  }